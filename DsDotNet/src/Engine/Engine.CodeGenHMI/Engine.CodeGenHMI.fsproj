<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<!--<TargetFramework>netcoreapp3.1</TargetFramework>-->
		<TargetFramework>net7.0-windows</TargetFramework>
		<RootNamespace>Engine.CodeGenHMI</RootNamespace>
		<LangVersion>7.0</LangVersion>
		<OutputType>Library</OutputType>
		<OutputPath>$(SolutionDir)\bin\</OutputPath>
		<OtherFlags>--warnon:1182</OtherFlags>
	</PropertyGroup>

  <ItemGroup>
    <Compile Include="CodeControlServer.fs" />
    <Compile Include="CodeGen.Common.fs" />
    <Compile Include="HmiCode.fs" />
    <Compile Include="PilotCode.fs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Engine.Core\Engine.Core.fsproj" />
    <ProjectReference Include="..\Engine.Parser.FS\Engine.Parser.FS.fsproj" />
    <ProjectReference Include="..\Engine.Parser\Engine.Parser.csproj" />
	<Compile Include="DummyInitializer.fs" />
	<Compile Include="CodeGenHandler.fs" />
	<Content Include="FodyWeavers.xml" />
  </ItemGroup>

  

  <ItemGroup>
	  <PackageReference Include="Fody" Version="6.6.4">
		  <PrivateAssets>all</PrivateAssets>
		  <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
	  </PackageReference>
	  <PackageReference Include="ModuleInit.Fody" Version="2.1.1">
		  <PrivateAssets>all</PrivateAssets>
	  </PackageReference>
  </ItemGroup>
</Project>
