<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net8.0</TargetFrameworks>
        <RootNamespace>UnitTest.Nuget.Common.CS</RootNamespace>
        <LangVersion>12.0</LangVersion>
        <SatelliteResourceLanguages>en;ko</SatelliteResourceLanguages>
        <OutputPath>$(SolutionDir)\bin\</OutputPath>
        <PlatformTarget>AnyCPU</PlatformTarget>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
		<!--<PackageReference Include="NUnit" Version="4.2.2" />-->
		<PackageReference Include="System.Text.Json" Version="9.0.1" />
		<PackageReference Include="Microsoft.NET.Test.Sdk" Version="17.11.0" />
		<PackageReference Include="NUnit3TestAdapter" Version="4.6.0" />
		<PackageReference Include="coverlet.collector" Version="6.0.2">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>

		<PackageReference Include="System.Configuration.ConfigurationManager" Version="8.0.0" />
		<PackageReference Include="FsUnit" Version="6.0.0" />
		<PackageReference Include="FsUnit.xUnit" Version="6.0.0" />
		<PackageReference Include="xunit" Version="2.9.0" />

	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\..\Common\Dual.Common.Base.FS\Dual.Common.Base.FS.fsproj" />
	  <ProjectReference Include="..\..\Common\Dual.Common.Core.FS\Dual.Common.Core.FS.fsproj" />
	</ItemGroup>

</Project>
