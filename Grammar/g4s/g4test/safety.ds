[sys] L = {
    [flow] F = {
        Main = { T.Cp > T.Cm; }
        [safety] = {
            Main = {P.F.Sp; P.F.Sm}
            Main2 = {P.F.Sp; P.F.Sm}
        }
    }
}

[sys] P = {
    [flow] F = {
        Vp > Pp > Sp;
        Vm > Pm > Sm;

        Pp |> Sm;
        Pm |> Sp;
        Vp <||> Vm;
    }
}

[prop] = {
    [ safety ] = {
        L.F.Main = {P.F.Sp; P.F.Sm}
        L.F.Main2 = {P.F.Sp; P.F.Sm}
    }
}
