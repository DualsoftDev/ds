let genResxEntry() =
    for f in files do
        let stem = Path.GetFileNameWithoutExtension(f)
        printf $"""<data name="{stem}" type="System.Resources.ResXFileRef, System.Windows.Forms">
        <value>..\\Resources\\{f};System.Drawing.Bitmap, System.Drawing, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a</value>
      </data>
    """
let genDesignerEntry() =
    for f in files do
        let stem = Path.GetFileNameWithoutExtension(f)
        printf $"""internal static System.Drawing.Bitmap {stem} {{
    get {{
        object obj = ResourceManager.GetObject("{stem}", resourceCulture);
        return ((System.Drawing.Bitmap)(obj));
    }}
}}
"""
