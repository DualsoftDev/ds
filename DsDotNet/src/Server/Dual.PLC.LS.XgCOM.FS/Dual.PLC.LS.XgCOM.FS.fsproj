<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0-windows</TargetFramework>
		<SatelliteResourceLanguages>en;ko</SatelliteResourceLanguages>
		<LangVersion>7.0</LangVersion>
		<OutputType>Library</OutputType>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<RootNamespace>Dual.PLC.LS.XgCOM.FS</RootNamespace>
		<PackageId>DualSoft-PLC-LS-XgCOM-FS</PackageId>
		<PackageTags>XgCOM,PLC,LS.FS,Dual</PackageTags>
		<Description>Dual.PLC.LS.XgCOM.FS</Description>
		<Title>Dual.PLC.LS.XgCOM.FS</Title>
		<Version>0.1.1</Version>
		<Company>Dual Inc.</Company>
		<Authors>Dual Inc.</Authors>
		<OutputPath>$(SolutionDir)\bin\</OutputPath>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="Connect.fs" />
		<Compile Include="Monitoring.fs" />
		<Compile Include="ScanIO.fs" />
		<COMReference Include="XGCommLib">
		  <WrapperTool>tlbimp</WrapperTool>
		  <VersionMinor>0</VersionMinor>
		  <VersionMajor>1</VersionMajor>
		  <Guid>2cff650c-6df0-48ae-9b06-c8dd467e0cd3</Guid>
		  <Lcid>0</Lcid>
		  <Isolated>false</Isolated>
		  <EmbedInteropTypes>true</EmbedInteropTypes>
		</COMReference>
		<None Include="Snippets.fsx" />
	</ItemGroup>


	<ItemGroup>
		<PackageReference Include="DualSoft-Common-Core-FS" Version="0.1.3" />
		<PackageReference Include="DualSoft-PLC-LS-FS" Version="0.1.2" />
		<PackageReference Include="System.Reactive" Version="6.0.0" />
		<PackageReference Include="log4net" Version="2.0.15" />
	</ItemGroup>

	<Target Name="PostBuild" AfterTargets="PostBuildEvent">
		<Exec Command="echo ProjectDir: $(ProjectDir)" />
		<Exec Command="echo OutputPath: $(OutputPath)" />
	</Target>

</Project>
