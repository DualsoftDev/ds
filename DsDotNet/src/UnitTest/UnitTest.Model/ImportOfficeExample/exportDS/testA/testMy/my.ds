[sys ip = localhost] my = {
    [flow] STN01 = {
        WORK2 => WORK3 => WORK1 => WORK2;
        WORK1 = {
            "STN01_Pin_+" > "STN01_Pin_-";
        }
    }
    [jobs] = {
        "STN01_Pin_+" = { STN01_Pin."+"(_, _); }
        "STN01_Pin_-" = { STN01_Pin."-"(_, _); }
    }
    [device file="../lib/dev.ds"] STN01_Pin; 
}