[sys] My = {
    [flow] F = {
        Seg1 > Seg2;
        Seg1 = {
            Ap > Am;
        }
    }
    [flow] F2 = {
        F.Seg1 > Seg;
        Seg = {
            Ap > Am;
        }
    }
    [calls] = {
        Ap = { A."+"(%Q1, %I1); }
        Am = { A."-"(%Q2, %I2); }
    }

    [device file="cylinder.ds"] A;
}
