[sys] Double = {
    [flow] Flow = {
        PistonRET ||> SensorADV;
        PistonRET <||> PistonADV ||> SensorRET;
        ValveRET > PistonRET > SensorRET;
        ValveRET <||> ValveADV > PistonADV > SensorADV;
    }
    [interfaces] = {
        ADV = { Flow.ValveADV ~ Flow.SensorADV }
        RET = { Flow.ValveRET ~ Flow.SensorRET }
        RET <||> ADV;
    }
}