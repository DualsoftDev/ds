// flow 내에 정의된 annonymous task

[sys] L = {
    [flow] F = {
        Main = { Cp > global.Cm; }
        // flowTask.  flow 내에 정의된 task.  anonymous.  no id
        [task] = {
            Cp = {P.F.Vp ~ P.F.Sp}
            Cm = {P.F.Vm ~ P.F.Sm}
        }
    }

    // sysTask.  system 내에 정의된 task.  id 명을 갖는다.
    [task] global = {
        Cp = {P.F.Vp ~ P.F.Sp}
        Cm = {P.F.Vm ~ P.F.Sm}
    }

}

