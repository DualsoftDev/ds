[sys] MovingLifter1 = {
    [flow] F = {
		ForwardUp = { Fw > Up1, Up2, Up3, Up4; }
		BackwardDown = { Dn1, Dn2, Dn3, Dn4 > Bw; }
		ForwardUp <||> BackwardDown;
    }
    [interfaces] = {
        "FwUp" = { F.ForwardUp ~ F.ForwardUp }
        "BwDn" = { F.BackwardDown ~ F.BackwardDown }
        "FwUp" <||> "BwDn";
    }
    [jobs] = {
        Up1 = { A."+"(%I100, %Q100); }
        Up2 = { B."+"(%I101, %Q101); }
        Up3 = { C."+"(%I102, %Q102); }
        Up4 = { D."+"(%I103, %Q103); }
        Dn1 = { A."-"(%I104, %Q104); }
        Dn2 = { B."-"(%I105, %Q105); }
        Dn3 = { C."-"(%I106, %Q106); }
        Dn4 = { D."-"(%I107, %Q107); }
		Fw  = { M.fwd(_    , %Q108); }
		Bw  = { M.bwd(_    , %Q109); }
    }
    [device file="cylinder.ds"] A;
    [device file="cylinder.ds"] B;
    [device file="cylinder.ds"] C;
    [device file="cylinder.ds"] D;
    [device file="motor.ds"] M;
}