[sys ip = 192.168.0.1] My = {

    [flow] F = {        // GVT.Flow
        C1, C2 > C3, C4 |> C5;
        [aliases] = {
            Ap = { Ap1; Ap2; }
            Am = { Am1; Am2; }
            Bp = { Bp1; Bp2; }
            Bm = { Bm1; Bm2; }
            Main = { Main2; }
        }
        // Flow 내의 safety 는 지원하지 않음
        //[safety] = {
        //    Main = { F.Main.Ap1; F.R2; }
        //}
    }

    [calls] = {
        Ap = {A."+"(%Q1, %I1);}
        Am = {A."-"(%Q2, %I2);}
        Bp = {B."+"(%Q3, %I3);}
        Bm = {B."-"(%Q4, %I4);}
    }

    [device file="cylinder.ds"] A;
    [device file="cylinder.ds"] B;
    [external file="station.ds" ip="192.168.0.2"] C;
    [prop] = {
        // Global safety
        [safety] = {
            F.Main = { F.Main.Ap1; F.R2 }
        }
        [layouts] = {
            Ap = (1309,405,205,83)
        }
    }

    [emg] = {
        EMGBTN = { F; };
        //EmptyButton = {};
        //NonExistingFlowButton = { F1; };
    }
}
