[sys] Sensor = {
    [flow] Flow = {
        PositionRET <|> PositionADV |> PartOFF;
        PositionRET |> PartON;
        PositionRET > PartOFF;
        PositionADV > PartON;
    }
    [interfaces] = {
        MOVE = { Flow.PartON ~ Flow.PartON }
        REMOVE = { Flow.PartOFF ~ Flow.PartOFF }
        REMOVE <|> MOVE;
    }
}
