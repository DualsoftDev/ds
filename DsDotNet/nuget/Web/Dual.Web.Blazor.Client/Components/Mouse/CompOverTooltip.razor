@code {
    [Parameter] public string TargetElementId { get; set; }
    [Parameter] public string TooltipElementId { get; set; }

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await JsDual.AttachTooltipEvents(TargetElementId, TooltipElementId);
        }
    }
}