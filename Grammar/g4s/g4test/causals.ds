[sys] L = {
    [flow] F = {
        Main = {
            Ap > Am;

            Ap1, Bp1 > Bm1;

            /* Grouped */
            //{ Ap1; Bp1; } > Bm1
            //{ Ap1; Bp1; } > { Am1; Bm1; }
        }
        [aliases] = {
            Ap = { Ap1; Ap2; Ap3; }
            Am = { Am1; Am2; Am3; }
            Bp = { Bp1; Bp2; Bp3; }
            Bm = { Bm1; Bm2; Bm3; }
        }
    }
    [calls] = {
        Ap = { A."+"(%Q1, %I1); }
        Am = { A."-"(%Q2, %I2); }
        Bp = { B."+"(%Q3, %I3); }
        Bm = { B."-"(%Q4, %I4); }
    }

    [prop] = {
        [safety] = {
            F.Main = { Ap; Am; }
            Ap = { F.Main; }
        }
    }

    [device file="cylinder.ds"] A;
    [device file="cylinder.ds"] B;
}
