[sys] L = {
    [flow] F = {
        Main = {
            Ap > Am;
        }
        [aliases] = {
            Ap = { Ap1; Ap2; Ap3; }
            Am = { Am1; Am2; Am3; }
        }
    }
    [calls] = {
        Ap = { A.""+""(%Q1, %I1); }
        Am = { A.""-""(%Q2, %I2); }
    }

    [prop] = {
        [safety] = {
            F.Main = { Ap; Am; }
            Ap = { F.Main; }
        }
    }

    [device file=""cylinder.ds""] A;
}
